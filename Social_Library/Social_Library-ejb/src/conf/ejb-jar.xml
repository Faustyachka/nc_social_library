<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
  <enterprise-beans>
        <session>
            <display-name>DaoSessionBeanSB</display-name>
            <ejb-name>DaoSessionBean</ejb-name>
            <local-home>dao.DaoSessionBeanLocalHome</local-home>
            <local>dao.DaoSessionBeanLocal</local>
            <ejb-class>dao.DaoSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <display-name>NewSessionBeanSB</display-name>
            <ejb-name>NewSessionBean</ejb-name>
            <local-home>sociallibrary.test.NewSessionBeanLocalHome</local-home>
            <local>sociallibrary.test.NewSessionBeanLocal</local>
            <ejb-class>sociallibrary.test.NewSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>DaoSessionBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>NewSessionBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        </assembly-descriptor>
    </ejb-jar>
